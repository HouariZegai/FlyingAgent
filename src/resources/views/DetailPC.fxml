<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTreeTableView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>


<VBox spacing="30.0" stylesheets="@../css/detail-pc.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="resources.views.DetailPC">
   <children>
      <StackPane>
         <children>
            <Label styleClass="title" text="Detail PC" StackPane.alignment="TOP_CENTER" />
            <JFXButton styleClass="btn-close" text=" " StackPane.alignment="CENTER_RIGHT" />
         </children>
      </StackPane>
      <ScrollPane VBox.vgrow="ALWAYS">
         <content>
            <FlowPane hgap="20.0" prefHeight="239.0" prefWidth="865.0" rowValignment="TOP" style="-fx-background-color: #FFF; -fx-border-color: #FFF;" vgap="20.0">
               <children>
                  <VBox prefHeight="215.0" prefWidth="430.0" spacing="10.0" styleClass="cards">
                     <children>
                        <Label styleClass="cards-title" text="Operating System" />
                        <GridPane hgap="10.0">
                          <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="10.0" prefWidth="150.0" />
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                          </columnConstraints>
                          <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                           <children>
                              <Label styleClass="cards-key" text="Name" />
                              <Label styleClass="cards-key" text="Version" GridPane.rowIndex="1" />
                              <Label styleClass="cards-key" text="Architecture" GridPane.rowIndex="2" />
                              <Label styleClass="cards-key" text="User Name" GridPane.rowIndex="3" />
                              <Label styleClass="cards-key" text="Computer Name" GridPane.rowIndex="4" />
                              <Label styleClass="cards-value" text="Windows" GridPane.columnIndex="1" />
                              <Label styleClass="cards-value" text="10.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                              <Label styleClass="cards-value" text="32bit" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                              <Label styleClass="cards-value" text="HouariZEGAI" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                              <Label styleClass="cards-value" text="DELL Inspiron" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                           </children>
                        </GridPane>
                     </children>
                  </VBox>
                  <VBox spacing="10.0" styleClass="cards">
                     <children>
                        <Label styleClass="cards-title" text="CPU" />
                        <GridPane hgap="10.0">
                           <columnConstraints>
                              <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="10.0" prefWidth="180.0" />
                              <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                           </columnConstraints>
                           <rowConstraints>
                              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                           </rowConstraints>
                           <children>
                              <Label styleClass="cards-key" text="Processor ID" />
                              <Label styleClass="cards-key" text="Processor Architecture" GridPane.rowIndex="1" />
                              <Label styleClass="cards-key" text="Number cores" GridPane.rowIndex="2" />
                              <Label styleClass="cards-value" text="100000000111111" GridPane.columnIndex="1" />
                              <Label styleClass="cards-value" text="32bit" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                              <Label styleClass="cards-value" text="4" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                           </children>
                        </GridPane>
                     </children>
                  </VBox>
                  <VBox spacing="10.0" styleClass="cards">
                     <children>
                        <Label styleClass="cards-title" text="Memory" />
                        <GridPane hgap="10.0">
                           <columnConstraints>
                              <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="10.0" prefWidth="180.0" />
                              <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                           </columnConstraints>
                           <rowConstraints>
                              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                           </rowConstraints>
                           <children>
                              <Label styleClass="cards-key" text="Free Physical Memory" />
                              <Label styleClass="cards-key" text="Physical Memory Size" GridPane.rowIndex="1" />
                              <Label styleClass="cards-key" text="In Use Memory Size" GridPane.rowIndex="2" />
                              <Label styleClass="cards-value" text="100000000111111" GridPane.columnIndex="1" />
                              <Label styleClass="cards-value" text="1000000000000111" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                              <Label styleClass="cards-value" text="1111111111110" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                           </children>
                        </GridPane>
                     </children>
                  </VBox>
                  <VBox spacing="10.0" styleClass="cards">
                     <children>
                        <Label styleClass="cards-title" text="Network" />
                        <JFXTreeTableView prefHeight="200.0" prefWidth="400.0" />
                     </children>
                  </VBox>
                  <VBox spacing="10.0" styleClass="cards">
                     <children>
                        <Label styleClass="cards-title" text="Disks" />
                        <JFXTreeTableView prefHeight="200.0" prefWidth="400.0" />
                     </children>
                  </VBox>
               </children>
            </FlowPane>
         </content>
      </ScrollPane>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="5.0" />
   </padding>
</VBox>
